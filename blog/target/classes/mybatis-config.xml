<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
  <typeAliases>
	  <typeAlias alias="User" type="entity.User"/>
	  <typeAlias alias="Location" type="entity.Location"/>
	  <typeAlias alias="Blog" type="entity.Blog"/>
	  <typeAlias alias="Request" type="entity.Request"/>
	  <typeAlias alias="Status" type="entity.Status"/>
  </typeAliases>
  <mappers>
    <mapper resource="userMapper.xml"/>
    <mapper resource="locationMapper.xml"/>
    <mapper resource="blogMapper.xml"/>
    <mapper resource="requestMapper.xml"/>
    <mapper resource="statusMapper.xml"/>
  </mappers>
</configuration>